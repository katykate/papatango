    <!-- Contact -->
    <section id="contact">

      <div class="container">

      <%= flash[:notice] %>
      <%= form_with(url: "/simple_pages/thank_you") do |form| %>

        <div class="row">

          <div class="col-lg-12 text-center">
            <h2 class="section-heading text-uppercase">Contact Me</h2>
            <h3 class="section-subheading text-muted">I will replay as soon as I can.</h3>
          </div>

        </div>

        <div class="row">

          <div class="col-lg-12">
            <form id="contactForm">

              <div class="row">
                <div class="col-md-6">
                  <div class="form-group">
                    <%= form.label :name %>
                    <%= form.text_field :name, autofocus: true, required: true, class: "form-control", placeholder: "Enter Name" %>
                  </div>
                  <div class="form-group">
                    <%= form.label :email, "Email Address" %>
                    <%= form.text_field :email, autofocus: true, required: true, :pattern => '[^@]+@[^@]+\.[a-zA-Z]{2,6}', class: "form-control",placeholder: "Enter Email" %>
                  </div>
                  <div class="form-group">
                    <%= form.label :phone, "Phone Number" %>
                    <%= form.text_field :phone, autofocus: true, required: true, :pattern => '\A\d{3}-\d{3}-\d{4}\z', class: "form-control", placeholder: "Enter Phone Number" %>
                  </div>
                </div>

                <div class="col-md-6">
                  <div class="form-group" style="height: 350px;">
                    <%= form.label :message %>
                    <%= form.text_area :message, autofocus: true, required: true, class: "form-control", placeholder: "Enter Message", cols: "75", minlength: 20 %>
                  </div>
                 </div>
                </div>
            </div>

                <div class="clearfix"></div>
                <div class="col-lg-12 text-center">
                  <div id="success">
                    <%= form.submit "Send", class: "btn btn-primary btn-xl text-uppercase" %>
                  </div>
                </div>
               
            </form>
            <% end %>
          </div>
         
        </div>
        
      </div>

